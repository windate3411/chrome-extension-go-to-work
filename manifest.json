{
  "manifest_version": 2,
  "name": "go-to-work",
  "version": "1.7",
  "description": "stop browzing other pages",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ]
}